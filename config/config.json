{
  "algorithm-type-name": "TestStrategy",
  "algorithm-language": "Python",
  "algorithm-location": "../test_strategy.py",

  "parameters": {
    "stop_loss_pct": 0.03,
    "take_profit_pct": 0.08,
    "rsi_oversold": 30,
    "rsi_overbought": 70
  },

  "data-folder": "../data",

  "debugging": true,
  "debugging-method": "LocalCmdline",

  "log-handler": "ConsoleLogHandler",

  "messaging-handler": "QuantConnect.Messaging.Messaging",
  "job-queue-handler": "QuantConnect.Queues.JobQueue",
  "api-handler": "QuantConnect.Api.Api",

  "environments": {
    "backtesting": {
      "live-mode": false
    },
    "live-trading": {
      "live-mode": true,
      "setup-handler": "QuantConnect.Lean.Engine.Setup.BrokerageSetupHandler",
      "result-handler": "QuantConnect.Lean.Engine.Results.LiveTradingResultHandler",
      "data-feed-handler": "QuantConnect.Lean.Engine.DataFeeds.LiveTradingDataFeed",
      "real-time-handler": "QuantConnect.Lean.Engine.RealTime.LiveTradingRealTimeHandler",
      "transaction-handler": "QuantConnect.Lean.Engine.TransactionHandlers.BrokerageTransactionHandler"
    }
  },

  "comments": {
    "algorithm-type-name": "Name of the algorithm class in test_strategy.py",
    "parameters": "Strategy parameters that can be modified without changing code",
    "debugging": "Enable detailed logging for development",
    "environments": "Separate configurations for backtesting vs live trading"
  }
}
